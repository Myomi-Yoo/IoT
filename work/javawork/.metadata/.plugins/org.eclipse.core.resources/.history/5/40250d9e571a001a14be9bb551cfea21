package chap05;

import java.util.Random;

//况农合 page 23. 巩力4
public class Array2DExam04 {
	public static void main(String[] args) {
		Random rand = new Random();
		
		int[] arr3 = new int[5];
		int sum=0;
		double avg=0.0;
		
		for(int i=0;i<arr3.length;i++) {
			arr3[i] = rand.nextInt(10)+1;
		}
		
		for(int i=0;i<arr3.length;i++) {
			sum += arr3[i];
		}
		avg=(double)sum/arr3.length;
	}

}
