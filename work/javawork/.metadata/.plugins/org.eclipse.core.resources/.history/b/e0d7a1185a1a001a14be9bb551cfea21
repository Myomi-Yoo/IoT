package chap05;

public class ArrayExam04 {
	public static void main(String[] args) {
		int sum = 0;
		int num1 = Integer.parseInt(args[0]);
		
		for(int i=0;i<=100;i++) {
			if(100%num1==0) {
				sum += num1;
			}
		}
		System.out.println(sum);
		
/*		for(int i=1;i<args.length;i++) {
			
		}*/
/*		for(int i=0;i<args.length;i++) {
			args = new String[i];
			System.out.println(args[i]);
		}
		*/

	}

}
