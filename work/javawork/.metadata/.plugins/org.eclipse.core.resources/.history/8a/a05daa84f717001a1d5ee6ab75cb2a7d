package chap04;
//break문 연습 - break문이 선언된 블럭을 빠져나간다.
public class GuGu_Break {
	public static void main(String[] args) {
		for(int row=1;row<=9;row++) {
			for(int i=2;i<=9;i++) {
				if(row==5) {
					break;
				}
				System.out.print(i+"*"+row+"="+i*row+"\t");
			}
			System.out.println();
		}
	}

}
