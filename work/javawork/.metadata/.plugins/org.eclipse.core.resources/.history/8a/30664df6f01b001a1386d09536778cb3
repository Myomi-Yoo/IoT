package workbook;
// 况农合 page58. 巩力4 
public class Student02 {
	private String name;
	private int age;
	private int height;
	private int weight;
	
	public Student02() {
		
	}
	public Student02(String name,int age,int height,int weight) {
		this.name = name;
		this.age = age;
		this.height = height;
		this.weight = weight;
	}
}
