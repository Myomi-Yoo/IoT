package chap04;

public class ForExam03 {
	public static void main(String[] args) {
		int sum = 0;
		for(int i=1;i<=100;i++) {
			sum += i;
		}
		System.out.println("Че :"+sum);
		sum=0;
		
		for(int j=1;j<=100;j+=2) {
			sum += j;
		}
		System.out.println("ШІМіЧе :"+sum);
		sum=0;
		
		for(int z=0;z<=100;z+=2) {
			sum += z;
		}
		System.out.println("ТІМіЧе :"+sum);
	}

}
