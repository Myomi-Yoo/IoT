package chap05;
// 况农合 page 14. 巩力7
public class ArrayExam04 {
	public static void main(String[] args) {
		int sum = 0;
		int num1 = Integer.parseInt(args[0]);
		System.out.println("涝仿 抗1:"+args[0]);
		
		for(int i=1;i<=100;i++) {
			if(i%num1==0) {
				sum += i;
				System.out.print(i+"+");
			}
		}
		
		System.out.println("="+sum);
	}

}
